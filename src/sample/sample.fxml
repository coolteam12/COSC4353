<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.SplitPane?>
<?import javafx.scene.control.TreeView?>
<?import javafx.scene.layout.AnchorPane?>

<SplitPane fx:id="mainPane" style="-fx-background-color: #1b1b1b;" dividerPositions="0.2" maxHeight="-Infinity"
           maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" onKeyPressed="#toggleFileViewer"
           prefHeight="900.0" prefWidth="1440.0" xmlns="http://javafx.com/javafx/11.0.1"
           xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.SplitPaneController">
    <items>
        <AnchorPane fx:id="fileViewer" minHeight="0.0" minWidth="0.0" onMouseClicked="#openFileMenu" prefHeight="160.0" prefWidth="100.0">
            <children>
                <TreeView fx:id="filesList" prefHeight="422.0" prefWidth="285.0"/>
            </children>
        </AnchorPane>
        <AnchorPane fx:id="codePane" minHeight="0.0" minWidth="0.0" onMouseClicked="#openFileMenu" prefHeight="160.0" prefWidth="100.0"/>
    </items>
</SplitPane>
